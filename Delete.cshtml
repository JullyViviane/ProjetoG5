@model Modeloa . Cadastros . Fabricante
@ {
    Layout  =  " ~ / Views / Shared / _LayoutPage1.cshtml " ;
    ViewBag . Title  =  " Visualizando detalhes de um FABRICANTE " ;
}
< div  class = " panel panel-primary " >
    < div  class = " panel-header " >
        Dados do fabricante a ser marítima
    </ div >
    < div  class = " panel-body " >
        @ Html.Partial ( "~ / Areas / Cadastros / Visualizações / Fabricantes / _PartialEditContentPanel.cshtml", Modelo)
    </ div >
    < div  class = " panel-footer panel-info " >
        @ using ( Html . BeginForm ())
        {
            @ Html.AntiForgeryToken ()
            < input  type = " submit "  value = " Remover FABRICANTE "  class = " btn btn-danger " />
            @ Html.ActionLink ( " Retornar para a listagem " , " Index " , null ,
            novo  {  @class  =  " btn btn-info "  } )
        }
    </ div >
</ div >